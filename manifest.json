{
  
    "manifest_version": 2,
    "name": "SAT Vocabulary Assist",
    "version": "0.0.1",
    "description": "Adds the definition to common SAT vocabulary words",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
